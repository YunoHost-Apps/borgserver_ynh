#!/bin/bash

#=================================================
# GENERIC START
#=================================================
# IMPORT GENERIC HELPERS
#=================================================

source /usr/share/yunohost/helpers

#=================================================
# LOAD SETTINGS
#=================================================

app=$YNH_APP_INSTANCE_NAME
ssh_user=$(ynh_app_setting_get $app ssh_user)

#=================================================
# REMOVE DEPENDENCIES
#=================================================
ynh_remove_app_dependencies

#=================================================
# REMOVE FILES
#=================================================
userdel $ssh_user
